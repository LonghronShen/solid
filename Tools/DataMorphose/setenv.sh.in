#!/bin/bash

# Set up the environment. Needed before building or starting.

platform='unknown'
unamestr=`uname`
if [[ "$unamestr" == 'Linux' ]]; then
   platform='linux'
elif [[ "$unamestr" == 'Darwin' ]]; then
   platform='macos'
fi

alias DataMorphose.exe='mono @CMAKE_RUNTIME_OUTPUT_DIR@/DataMorphose.exe'
