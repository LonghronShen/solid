INSERT INTO `locales_source` VALUES ('2674',' modules/system/system.admin.inc:1779','default','The number of joins without keys that check for key usage after each row; should be zero.','none'),('2675',' modules/system/system.admin.inc:1780','default','The number of sorts done without using an index; should be zero.','none'),('2676',' modules/system/system.admin.inc:1781','default','The number of times a lock could be acquired immediately.','none'),('2677',' modules/system/system.admin.inc:1782','default','The number of times the server had to wait for a lock.','none'),('2678',' modules/system/system.admin.inc:1785','default','Query cache information','none'),('2679',' modules/system/system.admin.inc:1786','default','The MySQL query cache can improve performance of your site by storing the result of queries. Then, if an identical query is received later, the MySQL server retrieves the result from the query cache rather than parsing and executing the statement again.','none'),('2680',' modules/system/system.admin.inc:1788','default','The number of queries in the query cache.','none'),('2681',' modules/system/system.admin.inc:1789','default','The number of times MySQL found previous results in the cache.','none'),('2682',' modules/system/system.admin.inc:1790','default','The number of times MySQL added a query to the cache (misses).','none'),('2683',' modules/system/system.admin.inc:1791','default','The number of times MySQL had to remove queries from the cache because it ran out of memory. Ideally should be zero.','none'),('2684',' modules/system/system.admin.inc:1904','default','Show descriptions','6.16'),('2685',' modules/system/system.admin.inc:1904','default','Expand layout to include descriptions.','6.16'),('2686',' modules/system/system.admin.inc:1907','default','Hide descriptions','6.16'),('2687',' modules/system/system.admin.inc:1907','default','Compress layout by hiding descriptions.','6.16'),('2688',' modules/system/system.admin.inc:2023;2207','default','Ve