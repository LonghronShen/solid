INSERT INTO `locales_source` VALUES ('1794',' modules/menu/menu.admin.inc:54','default','There are no menu items yet.','none'),('1795',' modules/menu/menu.admin.inc:72 modules/menu/menu.module:235','default','disabled','6.16'),('1796',' modules/menu/menu.admin.inc:104','default','reset','none'),('1797',' modules/menu/menu.admin.inc:175','default','Menu item','none'),('1798',' modules/menu/menu.admin.inc:177;299','default','Expanded','none'),('1799',' modules/menu/menu.admin.inc:230 modules/menu/menu.module:358','default','Menu settings','6.16'),('1800',' modules/menu/menu.admin.inc:260','default','The path this menu item links to. This can be an internal Drupal path such as %add-node or an external URL such as %drupal. Enter %front to link to the front page.','none'),('1801',' modules/menu/menu.admin.inc:279 modules/menu/menu.module:385','default','Menu link title','6.16'),('1802',' modules/menu/menu.admin.inc:281','default','The link text corresponding to this item that should appear in the menu.','none'),('1803',' modules/menu/menu.admin.inc:289','default','The description displayed when hovering over a menu item.','none'),('1804',' modules/menu/menu.admin.inc:295','default','Menu items that are not enabled will not be listed in any menu.','none'),('1805',' modules/menu/menu.admin.inc:301','default','If selected and this menu item has children, the menu will always appear expanded.','none'),('1806',' modules/menu/menu.admin.inc:315 modules/menu/menu.module:401','default','The maximum depth for an item and all its children is fixed at !maxdepth. Some menu items may not be available as parents if selecting them would exceed this limit.','6.16'),('1807',' modules/menu/menu.admin.inc:323 modules/menu/menu.module:411','default','Optional. In the menu, the heavier items will sink and the lighter items will be positioned nearer the top.','6.16'),('1808',' modules/menu/menu.admin.inc:379 modules/menu/menu.module:302','default','There was an error saving the menu link.','none'),('1809',' modules/menu/menu.admin.inc:403','default','Menu name','none'),('1810',' modules/menu/menu.admin.inc:405','default','The machine-readable name of this menu. This text will be used for constructing the URL of the <em>menu overview</em> page for this menu. This name must contain only lowercase letters, numbers, and hyphens, and must be unique.','none');
INSERT INTO `locales_source` VALUES ('1811',' modules/menu/menu.admin.inc:460','default','Are you sure you want to delete the custom menu %title?','none'),('1812',' modules/menu/menu.admin.inc:492','default','The custom menu %title has been deleted.','none'),('1813',' modules/menu/menu.admin.inc:502','default','The menu name may only consist of lowercase letters, numbers, and hyphens.','none'),('1814',' modules/menu/menu.admin.inc:512','default','The menu already exists.','none'),('1815',' modules/menu/menu.admin.inc:564','default','Are you sure you want to delete the custom menu item %item?','none'),('1816',' modules/menu/menu.admin.inc:574','default','The menu item %title has been deleted.','none'),('1817',' modules/menu/menu.admin.inc:584','default','Are you sure you want to reset the item %item to its default values?','none'),('1818',' modules/menu/menu.admin.inc:584','default','Any customizations will be lost. This action cannot be undone.','none'),('1819',' modules/menu/menu.admin.inc:593','default','The menu item was reset to its default settings.','none'),('1820',' modules/menu/menu.admin.inc:603','default','The menu module allows on-the-fly creation of menu links in the content authoring forms. The following option sets the default menu in which a new link will be added.','none'),('1821',' modules/menu/menu.admin.inc:609','default','Default menu for content','none'),('1822',' modules/menu/menu.admin.inc:612','default','Choose the menu to be the default in the menu options in the content authoring form.','none'),('1823',' modules/menu/menu.admin.inc:616','default','No primary links','none'),('1824',' modules/menu/menu.admin.inc:619','default','Source for the primary links','none'),('1825',' modules/menu/menu.admin.inc:623','default','Select what should be displayed as the primary links.','none'),('1826',' modules/menu/menu.admin.inc:626','default','No secondary links','none'),('1827',' modules/menu/menu.admin.inc:629','default','Source for the secondary links','none'),('1828',' modules/menu/menu.admin.inc:633','default','Select what should be displayed as the secondary links. You can choose the same menu for secondary links as for primary links (currently %primary). If you do this, the children of the active primary menu link will be displayed as secondary links.','none');
INSERT INTO `locales_source` VALUES ('1829',' modules/menu/menu.admin.inc:493','default','Deleted custom menu %title and all its menu items.','none'),('1830',' modules/menu/menu.admin.inc:575','default','Deleted menu item %title.','none'),('1831',' modules/menu/menu.admin.inc:457','default','<strong>Warning:</strong> There is currently 1 menu item in %title. It will be deleted (system-defined items will be reset).','none'),('1832',' modules/menu/menu.admin.inc:457','default','<strong>Warning:</strong> There are currently @count menu items in %title. They will be deleted (system-defined items will be reset).','none'),('1833',' modules/menu/menu.admin.inc:505','default','The menu name can\'t be longer than 1 character.','none'),('1834',' modules/menu/menu.admin.inc:505','default','The menu name can\'t be longer than @count characters.','none'),('1835',' modules/menu/menu.module:21','default','The menu module provides an interface to control and customize Drupal\'s powerful menu system. Menus are a hierarchical collection of links, or menu items, used to navigate a website, and are positioned and displayed using Drupal\'s flexible block system. By default, three menus are created during installation: <em>Navigation</em>, <em>Primary links</em>, and <em>Secondary links</em>. The <em>Navigation</em> menu contains most links necessary for working with and navigating your site, and is often displayed in either the left or right sidebar. Most Drupal themes also provide support for <em>Primary links</em> and <em>Secondary links</em>, by displaying them in either the header or footer of each page. By default, <em>Primary links</em> and <em>Secondary links</em> contain no menu items but may be configured to contain custom menu items specific to your site.','6.16'),('1836',' modules/menu/menu.module:22','default','The <a href=\"@menu\">menus page</a> displays all menus currently available on your site. Select a menu from this list to add or edit a menu item, or to rearrange items within the menu. Create new menus using the <a href=\"@add-menu\">add menu page</a> (the block containing a new menu must also be enabled on the <a href=\"@blocks\">blocks administration page</a>).','6.16');
INSERT INTO `locales_source` VALUES ('1837',' modules/menu/menu.module:23','default','For more information, see the online handbook entry for <a href=\"@menu\">Menu module</a>.','6.16'),('1838',' modules/menu/menu.module:26','default','Menus are a collection of links (menu items) used to navigate a website. The menus currently available on your site are displayed below. Select a menu from this list to manage its menu items.','6.16'),('1839',' modules/menu/menu.module:28','default','Enter the name for your new menu. Remember to enable the newly created block in the <a href=\"@blocks\">blocks administration page</a>.','none'),('1840',' modules/menu/menu.module:30','default','To rearrange menu items, grab a drag-and-drop handle under the <em>Menu item</em> column and drag the items (or group of items) to a new location in the list. (Grab a handle by clicking and holding the mouse while hovering over a handle icon.) Remember that your changes will not be saved until you click the <em>Save configuration</em> button at the bottom of the page.','none'),('1841',' modules/menu/menu.module:32','default','Enter the title and path for your new menu item.','none'),('1842',' modules/menu/menu.module:372','default','Delete this menu item.','6.16'),('1843',' modules/menu/menu.module:387','default','The link text corresponding to this item that should appear in the menu. Leave blank if you do not wish to add this post to the menu.','6.16'),('1844',' modules/menu/menu.module:40','default','administer menu','none'),('1845',' modules/menu/menu.module:48','default','Menus','6.16'),('1846',' modules/menu/menu.module:49','default','Control your site\'s navigation menu, primary links and secondary links. as well as rename and reorganize menu items.','6.16'),('1847',' modules/menu/menu.module:57','default','List menus','6.16'),('1848',' modules/menu/menu.module:63','default','Add menu','6.16'),('1849',' modules/menu/menu.module:78','default','Customize menu','none'),('1850',' modules/menu/menu.module:88','default','List items','none'),('1851',' modules/menu/menu.module:94','default','Add item','none');
