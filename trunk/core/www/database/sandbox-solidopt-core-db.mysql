INSERT INTO `locales_source` VALUES ('3150',' modules/update/update.compare.inc:365','default','Release revoked','none'),('3151',' modules/update/update.compare.inc:366','default','Your currently installed release has been revoked, and is no longer available for download. Disabling everything included in this release or upgrading is strongly recommended!','none'),('3152',' modules/update/update.compare.inc:377','default','Release not supported','none'),('3153',' modules/update/update.compare.inc:378','default','Your currently installed release is now unsupported, and is no longer available for download. Disabling everything included in this release or upgrading is strongly recommended!','none'),('3154',' modules/update/update.compare.inc:529','default','Unknown release date','none'),('3155',' modules/update/update.compare.inc:541','default','Invalid info','none'),('3156',' modules/update/update.fetch.inc:14;57','default','Fetched information about all available new releases and updates.','none'),('3157',' modules/update/update.fetch.inc:17;61','default','Unable to fetch any information about available new releases and updates.','none'),('3158',' modules/update/update.fetch.inc:57;61 modules/update/update.module:0','default','update','6.16'),('3159',' modules/update/update.report.inc:30','default','Last checked: @time ago','none'),('3160',' modules/update/update.report.inc:30','default','Last checked: never','none'),('3161',' modules/update/update.report.inc:31','default','Check manually','none'),('3162',' modules/update/update.report.inc:48;48','default','ok','none'),('3163',' modules/update/update.report.inc:70','default','Security update required!','none'),('3164',' modules/update/update.report.inc:73 modules/update/update.module:259','default','Revoked!','none'),('3165',' modules/update/update.report.inc:76','default','Not supported!','none'),('3166',' modules/update/update.report.inc:79','default','Update available','none'),('3167',' modules/update/update.report