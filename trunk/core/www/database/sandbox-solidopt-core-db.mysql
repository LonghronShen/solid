INSERT INTO `locales_source` VALUES ('2415',' modules/search/search.install:87','default','The numeric score of the word, higher being more important.','none'),('2416',' modules/search/search.install:98','default','Stores search totals for words.','none'),('2417',' modules/search/search.install:101','default','Primary Key: Unique word in the search index.','none'),('2418',' modules/search/search.install:108','default','The count of the word in the index using Zipf\'s law to equalize the probability distribution.','none'),('2419',' modules/search/search.install:117','default','Stores items (like nodes) that link to other nodes, used to improve search scores for nodes that are frequently linked to.','none'),('2420',' modules/search/search.install:124','default','The {search_dataset}.sid of the searchable item containing the link to the node.','none'),('2421',' modules/search/search.install:131','default','The {search_dataset}.type of the searchable item containing the link to the node.','none'),('2422',' modules/search/search.install:138','default','The {node}.nid that this item links to.','none'),('2423',' modules/search/search.install:144','default','The text used to link to the {node}.nid.','none'),('2424',' modules/statistics/statistics.admin.inc:14,  modules/statistics/statistics.pages.inc:46','default','Timestamp','6.16'),('2425',' modules/statistics/statistics.admin.inc:29,  modules/statistics/statistics.pages.inc:25;56','default','details','none'),('2426',' modules/statistics/statistics.admin.inc:33;64;97;129,  modules/statistics/statistics.pages.inc:29;60','default','No statistics available.','6.16'),('2427',' modules/statistics/statistics.admin.inc:50;79;115','default','Hits','none'),('2428',' modules/statistics/statistics.admin.inc:52','default','Average page generation time','none'),('2429',' modules/statistics/statistics.admin.inc:53;81','default','Total page generation time','none'),('2430',' modules/statistics/statistics.admin.inc:60','default','%time m