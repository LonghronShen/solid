INSERT INTO `locales_source` VALUES ('1160',' modules/book/book.admin.inc:15','default','edit order and titles','6.16'),('1161',' modules/book/book.admin.inc:33','default','Allowed book outline types','6.16'),('1162',' modules/book/book.admin.inc:36','default','Select content types which users with the %add-perm permission will be allowed to add to the book hierarchy. Users with the %outline-perm permission can add all content types.','6.16'),('1163',' modules/book/book.admin.inc:36\r modules/book/book.module:43','default','add content to books','6.16'),('1164',' modules/book/book.admin.inc:36\r modules/book/book.module:43','default','administer book outlines','6.16'),('1165',' modules/book/book.admin.inc:41','default','Default child page type','6.16'),('1166',' modules/book/book.admin.inc:44;60','default','The content type for the %add-child link must be one of those selected as an allowed book outline type.','6.16'),('1167',' modules/book/book.admin.inc:44;60\r modules/book/book.module:57','default','Add child page','6.16'),('1168',' modules/book/book.admin.inc:78','default','Save book pages','none'),('1169',' modules/book/book.admin.inc:116','default','Title changed from %original to %current.','none'),('1170',' modules/book/book.admin.inc:123','default','Updated book %title.','none'),('1171',' modules/book/book.admin.inc:118','default','book: updated %title.','none'),('1172',' modules/book/book.pages.inc:52','default','Unknown export format.','none'),('1173',' modules/book/book.pages.inc:126','default','Update book outline','none'),('1174',' modules/book/book.pages.inc:126','default','Add to book outline','6.16'),('1175',' modules/book/book.pages.inc:132','default','Remove from book outline','6.16'),('1176',' modules/book/book.pages.inc:160','default','No changes were made','none'),('1177',' modules/book/book.pages.inc:169','default','The post has been added to the selected book. You may now position it relative to other pages.','none'),('1178',' modules/book/book.pages.inc:173','default','The book outline has been updated.','6.16'),('1179',' modules/book/book.pages.inc:177','default','There was an error adding the post to the book.','none');
INSERT INTO `locales_source` VALUES ('1180',' modules/book/book.pages.inc:193','default','%title has associated child pages, which will be relocated automatically to maintain their connection to the book. To recreate the hierarchy (as it was before removing this page), %title may be added again using the Outline tab, and each of its former child pages will need to be relocated manually.','none'),('1181',' modules/book/book.pages.inc:196','default','%title may be added to hierarchy again using the Outline tab.','none'),('1182',' modules/book/book.pages.inc:199','default','Are you sure you want to remove %title from the book hierarchy?','none'),('1183',' modules/book/book.pages.inc:199','default','Remove','none'),('1184',' modules/book/book.pages.inc:213','default','The post has been removed from the book.','none'),('1185',' modules/book/book.module:64','default','Printer-friendly version','6.16'),('1186',' modules/book/book.module:66','default','Show a printer-friendly version of this book page and its sub-pages.','6.16'),('1187',' modules/book/book.module:180;189','default','Book navigation','6.16'),('1188',' modules/book/book.module:223','default','Show block on all pages','none'),('1189',' modules/book/book.module:224','default','Show block only on book pages','none'),('1190',' modules/book/book.module:228','default','Book navigation block display','none'),('1191',' modules/book/book.module:231','default','If <em>Show block on all pages</em> is selected, the block will contain the automatically generated menus for all of the site\'s books. If <em>Show block only on book pages</em> is selected, the block will contain only the one menu corresponding to the current page\'s book. In this case, if the current page is not in a book, no block will be displayed. The <em>Page specific visibility settings</em> or other visibility settings can be used in addition to selectively display this block.','none'),('1192',' modules/book/book.module:302','default','Change book (update list of parents)','6.16'),('1193',' modules/book/book.module:337','default','This is the top-level page in this book.','6.16');
INSERT INTO `locales_source` VALUES ('1194',' modules/book/book.module:340','default','This will be the top-level page in this book.','6.16'),('1195',' modules/book/book.module:344','default','No book selected.','6.16'),('1196',' modules/book/book.module:351','default','The parent page in the book. The maximum depth for a book and all child pages is !maxdepth. Some pages in the selected book may not be available as parents if selecting them would exceed this limit.','6.16'),('1197',' modules/book/book.module:369','default','Book outline','6.16'),('1198',' modules/book/book.module:391','default','Pages at a given level are ordered first by weight and then by title.','6.16'),('1199',' modules/book/book.module:408','default','create a new book','6.16'),('1200',' modules/book/book.module:422','default','Your page will be a part of the selected book.','6.16'),('1201',' modules/book/book.module:763','default','%title is part of a book outline, and has associated child pages. If you proceed with deletion, the child pages will be relocated automatically.','none'),('1202',' modules/book/book.module:1006','default','The book module is suited for creating structured, multi-page hypertexts such as site resource guides, manuals, and Frequently Asked Questions (FAQs). It permits a document to have chapters, sections, subsections, etc. Authors with suitable permissions can add pages to a collaborative book, placing them into the existing document by adding them to a table of contents menu.','6.16'),('1203',' modules/book/book.module:1007','default','Pages in the book hierarchy have navigation elements at the bottom of the page for moving through the text. These links lead to the previous and next pages in the book, and to the level above the current page in the book\'s structure. More comprehensive navigation may be provided by enabling the <em>book navigation block</em> on the <a href=\"@admin-block\">blocks administration page</a>.','6.16'),('1204',' modules/book/book.module:1008','default','Users can select the <em>printer-friendly version</em> link visible at the bottom of a book page to generate a printer-friendly display of the page and all of its subsections. ','6.16');
INSERT INTO `locales_source` VALUES ('1205',' modules/book/book.module:1009','default','Users with the <em>administer book outlines</em> permission can add a post of any content type to a book, by selecting the appropriate book while editing the post or by using the interface available on the post\'s <em>outline</em> tab.','6.16'),('1206',' modules/book/book.module:1011','default','For more information, see the online handbook entry for <a href=\"@book\">Book module</a>.','6.16'),('1207',' modules/book/book.module:1014','default','The book module offers a means to organize a collection of related posts, collectively known as a book. When viewed, these posts automatically display links to adjacent book pages, providing a simple navigation system for creating and reviewing structured content.','6.16'),('1208',' modules/book/book.module:1016','default','The outline feature allows you to include posts in the <a href=\"@book\">book hierarchy</a>, as well as move them within the hierarchy or to <a href=\"@book-admin\">reorder an entire book</a>.','6.16'),('1209',' modules/book/book.module:43','default','create new books','none'),('1210',' modules/book/book.module:43','default','access printer-friendly version','none'),('1211',' modules/book/book.module:79;108','default','Books','6.16'),('1212',' modules/book/book.module:80','default','Manage your site\'s book outlines.','6.16'),('1213',' modules/book/book.module:99','default','Re-order book pages and change titles','none'),('1214',' modules/book/book.module:122','default','Outline','6.16'),('1215',' modules/book/book.module:132','default','Remove from outline','none'),('1216',' modules/book/book.module:0','default','book','6.16'),('1217',' modules/book/book.install:29','default','Book page','6.16'),('1218',' modules/book/book.install:31','default','A <em>book page</em> is a page of content, organized into a collection of related entries collectively known as a <em>book</em>. A <em>book page</em> automatically displays links to adjacent pages, providing a simple navigation system for organizing and reviewing structured content.','6.16');
INSERT INTO `locales_source` VALUES ('1219',' modules/book/book.install:254','default','Stores book outline information. Uniquely connects each node in the outline to a link in {menu_links}','none'),('1220',' modules/book/book.install:261','default','The book page\'s {menu_links}.mlid.','none'),('1221',' modules/book/book.install:268','default','The book page\'s {node}.nid.','none'),('1222',' modules/book/book.install:275','default','The book ID is the {book}.nid of the top-level page.','none'),('1223',' modules/color/color.module:10','default','The color module allows a site administrator to quickly and easily change the color scheme of certain themes. Although not all themes support color module, both Garland (the default theme) and Minnelli were designed to take advantage of its features. By using color module with a compatible theme, you can easily change the color of links, backgrounds, text, and other theme elements. Color module requires that your <a href=\"@url\">file download method</a> be set to public.','none'),('1224',' modules/color/color.module:11','default','It is important to remember that color module saves a modified copy of the theme\'s specified stylesheets in the files directory. This means that if you make any manual changes to your theme\'s stylesheet, you must save your color settings again, even if they haven\'t changed. This causes the color module generated version of the stylesheets in the files directory to be recreated using the new version of the original file.','none'),('1225',' modules/color/color.module:12','default','To change the color settings for a compatible theme, select the \"configure\" link for the theme on the <a href=\"@themes\">themes administration page</a>.','none'),('1226',' modules/color/color.module:13','default','For more information, see the online handbook entry for <a href=\"@color\">Color module</a>.','none'),('1227',' modules/color/color.module:38','default','The color picker only works if the <a href=\"@url\">download method</a> is set to public.','none'),('1228',' modules/color/color.module:43','default','Color scheme','none');
