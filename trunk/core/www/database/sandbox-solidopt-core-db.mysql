INSERT INTO `locales_source` VALUES ('1012',' modules/block/block.admin.inc:174','default','\'%name\' block','none'),('1013',' modules/block/block.admin.inc:180','default','User specific visibility settings','none'),('1014',' modules/block/block.admin.inc:185','default','Custom visibility settings','none'),('1015',' modules/block/block.admin.inc:187','default','Users cannot control whether or not they see this block.','none'),('1016',' modules/block/block.admin.inc:188','default','Show this block by default, but let individual users hide it.','none'),('1017',' modules/block/block.admin.inc:189','default','Hide this block by default but let individual users show it.','none'),('1018',' modules/block/block.admin.inc:191','default','Allow individual users to customize the visibility of this block in their account settings.','none'),('1019',' modules/block/block.admin.inc:208','default','Role specific visibility settings','none'),('1020',' modules/block/block.admin.inc:213','default','Show block for specific roles','none'),('1021',' modules/block/block.admin.inc:216','default','Show this block only for the selected role(s). If you select no roles, the block will be visible to all users.','none'),('1022',' modules/block/block.admin.inc:221','default','Page specific visibility settings','none'),('1023',' modules/block/block.admin.inc:232','default','Show on every page except the listed pages.','none'),('1024',' modules/block/block.admin.inc:232','default','Show on only the listed pages.','none'),('1025',' modules/block/block.admin.inc:233','default','Enter one page per line as Drupal paths. The \'*\' character is a wildcard. Example paths are %blog for the blog page and %blog-wildcard for every personal blog. %front is the front page.','none'),('1026',' modules/block/block.admin.inc:236','default','Show if the following PHP code returns <code>TRUE</code> (PHP-mode, experts only).','none'),('1027',' modules/block/block.admin.inc:237','default','If the PHP-mode is chosen, en