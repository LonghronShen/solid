INSERT INTO `locales_source` VALUES ('2305',' modules/profile/profile.admin.inc:297','default','The specified form name is reserved for use by Drupal.','none'),('2306',' modules/profile/profile.admin.inc:304','default','The specified category name is reserved for use by Drupal.','none'),('2307',' modules/profile/profile.admin.inc:316','default','The specified title is already in use.','none'),('2308',' modules/profile/profile.admin.inc:319','default','The specified name is already in use.','none'),('2309',' modules/profile/profile.admin.inc:323','default','A hidden field cannot be required.','none'),('2310',' modules/profile/profile.admin.inc:326','default','A hidden field cannot be set to visible on the user registration form.','none'),('2311',' modules/profile/profile.admin.inc:344','default','The field has been created.','none'),('2312',' modules/profile/profile.admin.inc:350','default','The field has been updated.','none'),('2313',' modules/profile/profile.admin.inc:372','default','Are you sure you want to delete the field %field?','none'),('2314',' modules/profile/profile.admin.inc:373','default','This action cannot be undone. If users have entered values into this field in their profile, these entries will also be deleted. If you want to keep the user-entered data, instead of deleting the field you may wish to <a href=\"@edit-field\">edit this field</a> and change it to a hidden profile field so that it may only be accessed by administrators.','none'),('2315',' modules/profile/profile.admin.inc:386','default','The field %field has been deleted.','none'),('2316',' modules/profile/profile.admin.inc:345;387 modules/profile/profile.module:0','default','profile','6.16'),('2317',' modules/profile/profile.admin.inc:345','default','Profile field %field added under category %category.','none'),('2318',' modules/profile/profile.admin.inc:387','default','Profile field %field deleted.','none'),('2319',' modules/profile/profile.pages.inc:102 modules/profile/profile.module:80','default','User list','6.16'),('2320',' modules/profile/profile.module:35','default','The profile module allows custom fields (such as country, full name, or age) to be defined and displayed in the <em>My Account</em> section. This permits users of a site to share more information about themselves, and can help community-based sites organize users around specific information.','6.16');
INSERT INTO `locales_source` VALUES ('2321',' modules/profile/profile.module:36','default','The following types of fields can be added to a user profile:','6.16'),('2322',' modules/profile/profile.module:37;541','default','single-line textfield','6.16'),('2323',' modules/profile/profile.module:38;542','default','multi-line textfield','6.16'),('2324',' modules/profile/profile.module:39;543','default','checkbox','6.16'),('2325',' modules/profile/profile.module:40;544','default','list selection','6.16'),('2326',' modules/profile/profile.module:41;545','default','freeform list','6.16'),('2327',' modules/profile/profile.module:43;547','default','date','6.16'),('2328',' modules/profile/profile.module:44','default','For more information, see the online handbook entry for <a href=\"@profile\">Profile module</a>.','6.16'),('2329',' modules/profile/profile.module:47','default','This page displays a list of the existing custom profile fields to be displayed on a user\'s <em>My Account</em> page. To provide structure, similar or related fields may be placed inside a category. To add a new category (or edit an existing one), edit a profile field and provide a new category name. To change the category of a field or the order of fields within a category, grab a drag-and-drop handle under the Title column and drag the field to a new location in the list. (Grab a handle by clicking and holding the mouse while hovering over a handle icon.) Remember that your changes will not be saved until you click the <em>Save configuration</em> button at the bottom of the page.','none'),('2330',' modules/profile/profile.module:137','default','Author information','6.16'),('2331',' modules/profile/profile.module:148','default','Link to full user profile','none'),('2332',' modules/profile/profile.module:150','default','Profile fields to display','none'),('2333',' modules/profile/profile.module:153','default','Select which profile fields you wish to display in the block. Only fields designated as publ