INSERT INTO `locales_source` VALUES ('1613',' modules/forum/forum.admin.inc:38','default','Forum name','none'),('1614',' modules/forum/forum.admin.inc:41','default','Short but meaningful name for this collection of threaded discussions.','none'),('1615',' modules/forum/forum.admin.inc:47','default','Description and guidelines for discussions within this forum.','none'),('1616',' modules/forum/forum.admin.inc:54','default','Forums are displayed in ascending order by weight (forums with equal weights are displayed alphabetically).','none'),('1617',' modules/forum/forum.admin.inc:75','default','forum container','none'),('1618',' modules/forum/forum.admin.inc:79 modules/forum/forum.module:0','default','forum','none'),('1619',' modules/forum/forum.admin.inc:90','default','Created new @type %term.','none'),('1620',' modules/forum/forum.admin.inc:93','default','The @type %term has been updated.','none'),('1621',' modules/forum/forum.admin.inc:116','default','Container name','none'),('1622',' modules/forum/forum.admin.inc:120','default','Short but meaningful name for this collection of related forums.','none'),('1623',' modules/forum/forum.admin.inc:128','default','Description and guidelines for forums within this container.','none'),('1624',' modules/forum/forum.admin.inc:136','default','Containers are displayed in ascending order by weight (containers with equal weights are displayed alphabetically).','none'),('1625',' modules/forum/forum.admin.inc:168','default','Are you sure you want to delete the forum %name?','none'),('1626',' modules/forum/forum.admin.inc:168','default','Deleting a forum or container will also delete its sub-forums, if any. To delete posts in this forum, visit <a href=\"@content\">content administration</a> first. This action cannot be undone.','none'),('1627',' modules/forum/forum.admin.inc:176','default','The forum %term and all sub-forums and associated posts have been deleted.','none'),('1628',' modules/forum/forum.admin.inc:192','default','Hot topic threshold','none'),('1629',' modules/forum/forum.admin.inc:195','default','The number of posts a topic must have to be considered \"hot\".','none');
INSERT INTO `locales_source` VALUES ('1630',' modules/forum/forum.admin.inc:199','default','Topics per page','none'),('1631',' modules/forum/forum.admin.inc:202','default','Default number of forum topics displayed per page.','none'),('1632',' modules/forum/forum.admin.inc:204','default','Posts - most active first','none'),('1633',' modules/forum/forum.admin.inc:204','default','Posts - least active first','none'),('1634',' modules/forum/forum.admin.inc:206','default','Default order','none'),('1635',' modules/forum/forum.admin.inc:209','default','Default display order for topics.','none'),('1636',' modules/forum/forum.admin.inc:230','default','edit container','none'),('1637',' modules/forum/forum.admin.inc:233','default','edit forum','none'),('1638',' modules/forum/forum.admin.inc:245','default','There are no existing containers or forums. Containers and forums may be added using the <a href=\"@container\">add container</a> and <a href=\"@forum\">add forum</a> pages.','none'),('1639',' modules/forum/forum.admin.inc:286','default','Containers are usually placed at the top (root) level, but may also be placed inside another container or forum.','none'),('1640',' modules/forum/forum.admin.inc:289','default','Forums may be placed at the top (root) level, or inside another container or forum.','none'),('1641',' modules/forum/forum.admin.inc:177','default','forum: deleted %term and all its sub-forums and associated posts.','none'),('1642',' modules/forum/forum.module:16','default','A threaded discussion occurs as people leave comments on a forum topic (or on other comments within that topic). A forum topic is contained within a forum, which may hold many similar or related forum topics. Forums are (optionally) nested within a container, which may hold many similar or related forums. Both containers and forums may be nested within other containers and forums, and provide structure for your message board. By carefully planning this structure, you make it easier for users to find and comment on a specific forum topic.','none'),('1643',' modules/forum/forum.module:17','default','When administering a forum, note that:','none');
INSERT INTO `locales_source` VALUES ('1644',' modules/forum/forum.module:18','default','a forum topic (and all of its comments) may be moved between forums by selecting a different forum while editing a forum topic.','none'),('1645',' modules/forum/forum.module:19','default','when moving a forum topic between forums, the <em>Leave shadow copy</em> option creates a link in the original forum pointing to the new location.','none'),('1646',' modules/forum/forum.module:20','default','selecting <em>Read only</em> under <em>Comment settings</em> while editing a forum topic will lock (prevent new comments) on the thread.','none'),('1647',' modules/forum/forum.module:21','default','selecting <em>Disabled</em> under <em>Comment settings</em> while editing a forum topic will hide all existing comments on the thread, and prevent new ones.','none'),('1648',' modules/forum/forum.module:22','default','For more information, see the online handbook entry for <a href=\"@forum\">Forum module</a>.','none'),('1649',' modules/forum/forum.module:25','default','This page displays a list of existing forums and containers. Containers (optionally) hold forums, and forums hold forum topics (a forum topic is the initial post to a threaded discussion). To provide structure, both containers and forums may be placed inside other containers and forums. To rearrange forums and containers, grab a drag-and-drop handle under the <em>Name</em> column and drag the forum or container to a new location in the list. (Grab a handle by clicking and holding the mouse while hovering over a handle icon.) Remember that your changes will not be saved until you click the <em>Save</em> button at the bottom of the page.','none'),('1650',' modules/forum/forum.module:27','default','By grouping related or similar forums, containers help organize forums. For example, a container named \"Food\" may hold two forums named \"Fruit\" and \"Vegetables\", respectively.','none'),('1651',' modules/forum/forum.module:29','default','A forum holds related or similar forum topics (a forum topic is the initial post to a threaded discussion). For example, a forum named \"Fruit\" may contain forum topics titled \"Apples\" and \"Bananas\", respectively.','none');
INSERT INTO `locales_source` VALUES ('1652',' modules/forum/forum.module:31','default','These settings allow you to adjust the display of your forum topics. The content types available for use within a forum may be selected by editing the <em>Content types</em> on the <a href=\"@forum-vocabulary\">forum vocabulary page</a>.','none'),('1653',' modules/forum/forum.module:230','default','The item %forum is only a container for forums. Please select one of the forums below it.','none'),('1654',' modules/forum/forum.module:296','default','Forum topic','none'),('1655',' modules/forum/forum.module:298','default','A <em>forum topic</em> is the initial post to a new discussion thread within a forum.','none'),('1656',' modules/forum/forum.module:361','default','This is the designated forum vocabulary. Some of the normal vocabulary options have been removed.','none'),('1657',' modules/forum/forum.module:403;419','default','Active forum topics','none'),('1658',' modules/forum/forum.module:404;426','default','New forum topics','none'),('1659',' modules/forum/forum.module:408','default','Number of topics','none'),('1660',' modules/forum/forum.module:435','default','Read the latest forum topics.','none'),('1661',' modules/forum/forum.module:454','default','Leave shadow copy','none'),('1662',' modules/forum/forum.module:454','default','If you move this topic, you can leave a link in the old forum to the new forum.','none'),('1663',' modules/forum/forum.module:561','default','Topic','none'),('1664',' modules/forum/forum.module:563','default','Created','none'),('1665',' modules/forum/forum.module:564','default','Last reply','none'),('1666',' modules/forum/forum.module:677','default','Post new @node_type','none'),('1667',' modules/forum/forum.module:684','default','You are not allowed to post new content in forum.','none'),('1668',' modules/forum/forum.module:688','default','<a href=\"@login\">Login</a> to post new content in forum.','none'),('1669',' modules/forum/forum.module:726','