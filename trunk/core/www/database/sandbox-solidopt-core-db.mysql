INSERT INTO `locales_source` VALUES ('3169',' modules/update/update.report.inc:149','default','Latest version:','none'),('3170',' modules/update/update.report.inc:155','default','Development version:','none'),('3171',' modules/update/update.report.inc:161','default','Also available:','none'),('3172',' modules/update/update.report.inc:181','default','Includes: %includes','none'),('3173',' modules/update/update.report.inc:196','default','Drupal core','none'),('3174',' modules/update/update.report.inc:199','default','Disabled modules','none'),('3175',' modules/update/update.report.inc:200','default','Disabled themes','none'),('3176',' modules/update/update.report.inc:229','default','Download','none'),('3177',' modules/update/update.report.inc:233','default','Release notes','none'),('3178',' modules/update/update.settings.inc:18','default','E-mail addresses to notify when updates are available','none'),('3179',' modules/update/update.settings.inc:21','default','Whenever your site checks for available updates and finds new releases, it can notify a list of users via e-mail. Put each address on a separate line. If blank, no e-mails will be sent.','none'),('3180',' modules/update/update.settings.inc:26','default','Check for updates','none'),('3181',' modules/update/update.settings.inc:29','default','Daily','none'),('3182',' modules/update/update.settings.inc:30','default','Weekly','none'),('3183',' modules/update/update.settings.inc:32','default','Select how frequently you want to automatically check for new releases of your currently installed modules and themes.','none'),('3184',' modules/update/update.settings.inc:37','default','Notification threshold','none'),('3185',' modules/update/update.settings.inc:40','default','All newer versions','none'),('3186',' modules/update/update.settings.inc:41','default','Only security updates','none'),('3187',' modules/update/update.settings.inc:43','default','If there are updates available of Drupal core or any of your installed modules and themes, your site will print an error message on the <a href=\"@status_report\">status report</a>, the <a href=\"@modules_page\">modules page</a>, and the <a href=\"@themes_page\">themes page</a>. You can choose to only see these error messages if a security update is available, or to be notified about any newer versions.','none');
INSERT INTO `locales_source` VALUES ('3188',' modules/update/update.settings.inc:80','default','%email is not a valid e-mail address.','none'),('3189',' modules/update/update.settings.inc:83','default','%emails are not valid e-mail addresses.','none'),('3190',' modules/update/update.module:61','default','Here you can find information about available updates for your installed modules and themes. Note that each module or theme is part of a \"project\", which may or may not have the same name, and might include multiple modules or themes within it.','none'),('3191',' modules/update/update.module:62','default','To extend the functionality or to change the look of your site, a number of contributed <a href=\"@modules\">modules</a> and <a href=\"@themes\">themes</a> are available.','none'),('3192',' modules/update/update.module:79','default','See the <a href=\"@available_updates\">available updates</a> page for information on installed modules and themes with new versions released.','6.16'),('3193',' modules/update/update.module:87','default','The Update status module periodically checks for new versions of your site\'s software (including contributed modules and themes), and alerts you to available updates.','6.16'),('3194',' modules/update/update.module:88','default','The <a href=\"@update-report\">report of available updates</a> will alert you when new releases are available for download. You may configure options for update checking frequency and notifications at the <a href=\"@update-settings\">Update status module settings page</a>.','6.16'),('3195',' modules/update/update.module:89','default','Please note that in order to provide this information, anonymous usage statistics are sent to drupal.org. If desired, you may disable the Update status module from the <a href=\"@modules\">module administration page</a>.','6.16'),('3196',' modules/update/update.module:90','default','For more information, see the online handbook entry for <a href=\"@update\">Update status module</a>.','6.16'),('3197',' modules/update/update.module:211;243','default','Drupal core update status','6.16');
