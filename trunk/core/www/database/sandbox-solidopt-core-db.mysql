INSERT INTO `locales_source` VALUES ('4603','sites/default/modules/syntaxhighlighter/syntaxhighlighter.min.js','default','Can\\\'t find brush for: ','none'),('4604','sites/default/modules/syntaxhighlighter/syntaxhighlighter.min.js','default','Brush wasn\\\'t made for html-script option: ','none'),('4605','/bg/content/%D0%BC%D0%BE%D0%B4%D0%B5%D0%BB%D0%B8-0','default','Syntax Highlighter','6.16'),('4606','/bg/admin','default','Syntax highlighter js library','6.16'),('4607','/bg/admin','default','Installed','6.16'),('4608','/bg/admin','default','Syntax highlighter','6.16'),('4609','/bg/admin','default','Configure syntax highlighter','6.16'),('4610','/bg/content/%D0%BF%D0%BE%D0%B4%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B8-%D1%81%D0%B2%D1%8A%D1%80%D0%B7%D0%B0%D0%BD%D0%B8-%D1%81%D1%8A%D1%81-solidopt','default','Display a printer-friendly version of this page.','6.16'),('4611','user:1:title','blocks','<rounded>','1'),('4612','menu:secondary-links:title','blocks','<rounded>','1'),('4614','/bg/content/%D0%B4%D0%BE%D0%BA%D1%83%D0%BC%D0%B5%D0%BD%D1%82%D0%B0%D1%86%D0%B8%D1%8F','default','Toolbar for the main column','6.16'),('4613','user:0:title','blocks','<rounded>','1'),('4615','/bg/node/25','default','Sitemap for the footer','6.16');
DROP TABLE IF EXISTS `locales_target`;
CREATE TABLE `locales_target` (   `lid` int(11) NOT NULL DEFAULT '0',   `translation` blob NOT NULL,   `language` varchar(12) NOT NULL DEFAULT '',   `plid` int(11) NOT NULL DEFAULT '0',   `plural` int(11) NOT NULL DEFAULT '0',   `status` int(11) NOT NULL DEFAULT '0',   PRIMARY KEY (`language`,`lid`,`plural`),   KEY `lid` (`lid`),   KEY `plid` (`plid`),   KEY `plural` (`plural`) ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
INSERT INTO `locales_target` VALUES ('3060','Модулът Throttle следи за претоварване на сайта и автоматично реагира при внезапно повишаване на трафика. Ако на сайта ви е линкнат от популярен портал или сте под DoS атака, сървърът ви ще се претовари. Throttle модълът действа като временно деактивира функционалност изискваща много процесорни ресурси. Например, някои блокове изискващи много ресурси за да се гнерират могат да бъдат деактивирани или могат да бъдат деактивирани потребителските снимки в техните публикации.','bg','0','0','0'),('3059','Стойността %value е невалидна като настройка за авт. редуциране. Моля изберете положителна числена стойност.','bg','0','0','0'),('3058','Автоматичното редуциране с вероятностен ограничител представлява ефективен механизъм за статистическо ограничаване на пропуснатото от автоматичното редуциране. Ограничителят се изразява като процент от преглежданите страници. Така например, ако е настроен на 10% (както е по подразбиране), то сайта изпраща допълнителна заявка към базата данни с цел обновяване състоянието на авт. редуциране един път на всеки 10 прегледани страници. Колкото е по натоварен Вашия сайт, толкова по-малка трябва да е стойността на този ограничител.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('3057','Автоматично редуциране с вероятностен ограничител','bg','0','0','0'),('3056','Редуцирането при претоварване може да се активира автоматично, когато текущия брой на неоторизирани потребители, посещаващи сайта, надмине зададен праг. Например, за да стартирате редуцирането, когато сайта има 50 регистрирани потребители онлайн, въведете в това поле \'50\'. Оставете полето празно или въведете \"0\" ако не желаете автоматично редуциране при оторизирани потребители. Можете да проверите текущия брой на оторизираните потребители, използвайки блока \"Кой е онлайн\".','bg','0','0','0'),('3055','Автоматично редуциране при оторизирани потребители','bg','0','0','0'),('3054','Редуцирането при претоварване може да се активира автоматично, когато текущия брой на анонимни потребители, посещаващи сайта, надмине зададен праг. Например, за да стартирате редуцирането, когато сайта има 250 анонимни потребители свързани едновременно, въведете в това поле \'250\'. Оставете полето празно или въведете \"0\" ако не желаете автоматично редуциране при анонимни потребители. Можете да проверите текущия брой на анонимните потребители, използвайки блока \"Кой е онлайн\".','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('3053','Автоматично редуциране при анонимни потребители','bg','0','0','0'),('3052','{node}.type на типа на node за който може да се използва речника.','bg','0','0','0'),('3051','Първичен ключ: {vocabulary}.vid на речника.','bg','0','0','0'),('3050','Stores which node types vocabularies may be used with.','bg','0','0','0'),('3049','Важността (weight) на този речник във връзката му с другу речници.','bg','0','0','0'),('3048','Модула, който е създал този речник.','bg','0','0','0'),('3047','Дали е активирано free tagging за този речник. (0 = активирано, 1 = деактивирано)','bg','0','0','0'),('3046','Дали се изискват термини за nodes, когато се използва този речник. (0 = активирано, 1 = деактивирано)','bg','0','0','0'),('3045','Дали може множество термини от този речник могат да бъдат асоциирани към node. (0 = активиранo, 1 = деактивиранo)','bg','0','0','0'),('3044','Дали са активирани свързаните термини в рамките на речника. (0 = активирани, 1 = деактивирани)','bg','0','0','0'),('3043','Дали са активирани свързаните термини в рамките на речника. (0 = активирани, 1 = деактивирани)','bg','0','0','0'),('3042','Помощен текст при показване на речника.','bg','0','0','0'),('3041','Описание на речника.','bg','0','0','0'),('3039','Първичен ключ: Уникално ID на речника.','bg','0','0','0'),('3040','Име на речника.','bg','0','0','0'),('3036','{term_data}.tid на терминa.','bg','0','0','0'),('3037','Името на синонима.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('3038','Записва информация за речника.','bg','0','0','0'),('3035','Първичен ключ: Уникално ID на синонима на речника.','bg','0','0','0'),('3034','Записва синонимите.','bg','0','0','0'),('3033','{term_data}.tid на втория термин във връзката.','bg','0','0','0'),('3032','{term_data}.tid на първия термин във връзката.','bg','0','0','0'),('3031','Първичен ключ: Уникално ID на връзката на термина.','bg','0','0','0'),('3030','Зписва не-йерархичните връзки между термините.','bg','0','0','0'),('3029','Първичен ключ: {term_data}.tid на термина асоцииран към този node.','bg','0','0','0'),('3028','Първичен ключ: {node}.nid на node.','bg','0','0','0'),('3027','Записва връзката между термините и nodes.','bg','0','0','0'),('3026','Първичен ключ: {term_data}.tid на основния термина на термина.','bg','0','0','0'),('3025','Първичен ключ: {term_data}.tid на термина.','bg','0','0','0'),('3024','Записва йерархичните взаимовръзки между термините.','bg','0','0','0'),('3023','Важността (weight) на този термин във връзките му с други термини.','bg','0','0','0'),('3022','Описание на термини.','bg','0','0','0'),('3021','Име на термина.','bg','0','0','0'),('3020','{vocabulary}.vid на речника към което термина е асоцииран.','bg','0','0','0'),('3019','Първичен ключ: Уникален ID на термина.','bg','0','0','0'),('3018','Изпращане на системна информация','bg','0','0','0'),('3016','Подреди термините','bg','0','0','0'),('3017','Прибави термин','bg','0','0','0'),('3015','Autocomplete taxonomy','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('3014','Taxonomy термин (класификационен термин)','bg','0','0','0'),('3013','Редактирай термин','bg','0','0','0'),('3012','Редактирай речник','bg','0','0','0'),('3011','Прибави речник','bg','0','0','0'),('3010','Управлява tagging, категоризацията и класификацията на вашето съдържание.','bg','0','0','0'),('3009','администриране на taxonomy','bg','0','0','0'),('3008','След изтриването на термин','bg','0','0','0'),('3007','След записването на обновен термин в базата данни','bg','0','0','0'),('3006','След записването на нов термин в базата данни','bg','0','0','0'),('3005','Дефинира как вашия речник ще бъде представен на администраторите и потребителите, и кои типове съдържание ще се категоризират със него. Tags позволяват на потребителите да създават термини, когато изпращат публикации, чрез написването на списък, отделните елементи в който са разделени със запетаи. Иначе термините се избират от списък и могат да бъдат създавани само от потребители със разрешение за \"администриране на taxonomy\".','bg','0','0','0'),('3004','%capital_name е речник със множество йерархии. За да промените името или описанието на термин, щракнете на линка <em>редакция</em> отстрани на термина. Влачене-и-пускане на множество йерархии не се поддържа, но вие можете да активирате отново поддръжката му, като редактирате всеки термин, така, че да има само един основен термин.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('3003','%capital_name е речник със единична йерархия. Вие можете да организирате термините в речника %name, като използвате handler-те от лявата страна на таблицата. За да промените името или описанието на термин, щракнете на линка <em>редакция</em> отстрани на термина.','bg','0','0','0'),('3002','%capital_name е плосък речник. Можете да организирате термините р речника %name чрез използването на handle-и от лявата страна на таблицата. За да промените името или описанието на термин, щракнете на линка <em>редакция</em> отстрани на термина.','bg','0','0','0'),('3001','%capital_name е речник със free-tags. За да смените името или описанието на термин, щракнете върху линка <em>редакция</em> отстрани на термина. ','bg','0','0','0'),('3000','Използвайте списъка по-долу, за да конфигурирате и прегледате речниците, дефинирани във вашия сайт или за да изброите или управлявате термините (tags), които те съдържат. Речника може (опционно) да бъде свързан със дадени типове съдържание, както е показано в колоната <em>Тип</em>. Ако речника е свързан, името му ще бъде показвано когато се създават или редактират публикации, от типовете съдържание, с които е свързан. Множеството речници, свързани с един и същи тип съдържание ще бъдат показани в последователността, показана по-долу. За да промените подреждането в речник хванете влачи-и-пусни handle-ра, намиращ се на колоната <em>Име</em> и я влачете до новото положение в списъка. (Хванете handle-ра като щракнете и задършите показалеца на мишката върху иконата на handle-ра). Запомнете, че промените, които сте направили няма да бъдат записани, докато не щракнете върху бутона <em>Запис</em>, намиращ се на дъното на страницата.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2999','Модула taxonomy ви позволява да категоризирате своето съдържание използвайки едновременно tags и дефинирани от администратора термини. Той е гъвкав инструмент за класифициране на съдържание със много големи възможности. За начало, създайте \'Речник\', който ще съдържа едно множество от термини или от tags. Можете да създадете речник със free-tagging категории за всичко, или да създадете отделни контролирани речници, за да дефинирате различни свойства на своето съдържание, например \'Държави\'или \'Цветове\'.','bg','0','0','0'),('2998','За повече информация, моля прочетете наръчника за конфигуриране и персонализиране <a href=\"@taxonomy\">модул taxonomy</a>.','bg','0','0','0'),('2997','Модула taxonomy поддържа използването едновременно на синоними и на свързани термини, но не използва директно тази функционалност. Въпреки това, някои от опционно предлаганите модули може да използват пълните възможности на тази функционалност.','bg','0','0','0'),('2996','Термините също могат да бъдат организирани в във взаимоотношения от тип основен термин/подчинен термин. Пример за това е речник групиращ държавите по основни геополитически региони. Модула taxonomy може да се справи със прилагането на сложна йерархия, например слагайки Турция едновременно в \'Среден изток\' и в Европа\'.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2995','Всеки термин в taxonomy (често наричан \'категория\' или \'tag\' в други системи) автоматично предоставя списъци със публикации и свързаните със тях RSS канали. Тези taxonomy/term URL-и могат да бъдат манипулирани, за да генерират AND и OR списъци със публикации, класифицирани чрез термини. В примера със сайта за рецепти ще бъде лесно да се създадат страници показващи \'Основни манджи\', \'Рецепти за 30 мин.\' или \'Основни манджи и мезенца за 30 мин.\', чрез използването на собствени термини или на комбинации със други термини. Има значителен брой опционни модули, които можете да модифицирате и да разширявате функционалността им, за да постигнете едновременно показване и организиране на термините.','bg','0','0','0'),('2994','Време за приготвяне: <em>0-30 мин., 30-60 мин., 1-2 ч., 2+ ч.</em>','bg','0','0','0'),('2993','Тип на манджата: <em>Мезенце, Основна манджа, Салатка, Десертче</em>','bg','0','0','0'),('2992','Например, когато създавате сайт за рецепти, вие може да поискате да класифицирате публикациите едновременно по тип на манджата и по времето за приготвяне. Създаването на отделни речници за двата критерия, ще ви позволи да категоризирате всеки от критериите независимо от другия, вместо да създавате tag за всяка възможна комбинация.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2991','Модула taxonomy ви позволява да категоризирате съдържанието, използвайки различни системи за класификация. Речниците със free-tagging се създават от потребителите в движение, когато те изпращат публикации (както най-често се случва във блогове и в приложения за social bookmarking). Контролираните речници позволяват дефинирани от администраторите кратки списъци със термини, а така също и на сложни йерархии със много взаимовръзки между различните термини. Тези методи могат да се приложат към различни типове съдържание и да се комбинират помежду си с цел създаване на мощен и гъвкав метод за класифициране и представяне на вашето съдържание.','bg','0','0','0'),('2990','В момента няма постове в тази категория.','bg','0','0','0'),('2989','Речникът %name неможе да бъде променен по този начин.','bg','0','0','0'),('2988','Речници','bg','0','0','0'),('2985','- Няма избрани -','bg','0','0','0'),('2986','- Няма -','bg','0','0','0'),('2987','Списък от термини разделени със запетайки описва това съдържание. Например: забавен, скачане с бънджи, \"Company, Inc.\".','bg','0','0','0'),('2984','taxonomy','bg','0','0','0'),('2983','Връща речника %name в началното положение на подредба по азбучен ред.','bg','0','0','0'),('2982','Връщането на речника в начално положение ще отмени всички дефинирани от петребителя подредби и ще сортира елементите по азбучен ред.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2981','Сигурни ли сте че искате да върнете речника %title в началното положение на подредба по азбучен ред?','bg','0','0','0'),('2980','Изтрито име на речника %name.','bg','0','0','0'),('2979','Изтриването на речника ще изтрие всички термини в него. Това действие е окончателно.','bg','0','0','0'),('2977','Изтрито име на термин %name.','bg','0','0','0'),('2978','Сигурни ли сте че искате да изтриете речника %title?','bg','0','0','0'),('2976','Изтриването на термина ще изтрие и неговите производни ако има такива. Това действие е окончателно.','bg','0','0','0'),('2974','Настройва множество основни термини','bg','0','0','0'),('2975','Сигурни ли сте че искате да изтриете термина %title?','bg','0','0','0'),('2973','Можете отново да активирате интерфейса влачи-и-пусни по всяко време, чрез редуциране на множеството основни термини до един основен термин за термините в този речник.','bg','0','0','0'),('2968','Тегловата (weight) стойност трябва да бъде число.','bg','0','0','0'),('2969','Създаден е нов термин %term.','bg','0','0','0'),('2970','Обновен термин %term.','bg','0','0','0'),('2971','Желаете термина да има множество основни термини','bg','0','0','0'),('2972','Добавянето на множество основни термини към даден термин, ще накара речника %vocabulary да потърси множество основни термини за всеки термин. Понеже множеството основни термини не се поддържат, когато се използва интерфейс влачи-и-пусни, интерфейса от тизи тип ще бъде деактивиран, ако вие активирате тази опция. Ако изберете да имате множество основни термини, ще можете да настройвате основни термини само от формата за редактиране на термин.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2967','Термините се показват в нарастващ ред по важност.','bg','0','0','0'),('2955','термини в %vocabulary','bg','0','0','0'),('2956','Няма налични термини','bg','0','0','0'),('2957','Възстанови стойностите по подразбиране','bg','0','0','0'),('2958','Добави термин към %vocabulary','bg','0','0','0'),('2959','Име на термин','bg','0','0','0'),('2960','Името на този термин.','bg','0','0','0'),('2961','Описание на термина. Показва се в taxonomy/term страници и RSS канали.','bg','0','0','0'),('2962','Основи','bg','0','0','0'),('2963','Основни термини','bg','0','0','0'),('2964','Свързани термини (related terms)','bg','0','0','0'),('2965','Синоними','bg','0','0','0'),('2966','Синоними на този термин, по един синоним на ред.','bg','0','0','0'),('2954','Обновяване на името на речник %name.','bg','0','0','0'),('2953','Създаване на ново име на речник %name.','bg','0','0','0'),('2952','Речниците се показват във възходящ ред по важност.','bg','0','0','0'),('2951','Най-малко един термин в този речник трябва да бъде избран, когато се изпраща публикация.','bg','0','0','0'),('2949','Маркиране на няколко','bg','0','0','0'),('2950','Позволява публикациите да имат повече от един термин от този речник (винаги \'истина\' за tags).','bg','0','0','0'),('2948','Термините са създадени от потребители, когато изпратят публикации, чрез написването на списък, отделните елементи в който са разделени със запетаи.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2947','Категории','bg','0','0','0'),('2946','Изберете типовете съдържание, които да бъдат категоризирани с използване на този речник.','bg','0','0','0'),('2945','Инструкции които да бъдат показани на потребителя когато той избира термин. <em>\"Въведете списък от думи, разделени със запетаи\"</em>.','bg','0','0','0'),('2944','Помощен текст','bg','0','0','0'),('2943','Описание на речника; може да се използва от модули.','bg','0','0','0'),('2937','редакция на речник','bg','0','0','0'),('2938','показване на термините','bg','0','0','0'),('2939','добавяне на термин','bg','0','0','0'),('2940','Няма достъпни речници.','bg','0','0','0'),('2941','Име на речника','bg','0','0','0'),('2942','Името на този речник. Например: <em>\"Tags\"</em>.','bg','0','0','0'),('2936','The language this alias is for; if blank, the alias will be used for unknown languages. Each Drupal path can have an alias for each supported language.','bg','0','0','0'),('2934','The Drupal path this alias is for; e.g. node/12.','bg','0','0','0'),('2935','The alias for this path; e.g. title-of-the-story.','bg','0','0','0'),('2933','A unique path alias identifier.','bg','0','0','0'),('2932','A list of URL aliases for Drupal paths; a user may visit either the source or destination path.','bg','0','0','0'),('2931','Детайлен списък съдържащ информация от .info файла на модула; ключовете могат да съдържат име, описание, пакет, версия, ядро, зависимости, зависещи и php изисквания.','bg','0','0','0'),('2930','The order in which this module\'s hooks should be invoked relative to other modules. Equal-weighted modules are ordered by name.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2929','Версия на схемата на базата данни за модула. -1 ако модула не е инсталиран (неговата таблица несъществува); 0 или най-голямото N от hook_update_N() функция на модула, когато е била стартирана или съществувала при първоначална инсталация на модула.','bg','0','0','0'),('2928','Boolean индикиращ дали този модул е зареден по време на предишна Drupal фаза (т.е. преди да е отправена заявка за търсене в кеша).','bg','0','0','0'),('2926','Boolean индикиращ дали този обект е активен иле неактивен.','bg','0','0','0'),('2927','Boolean индикиращ дали този обект е неактивен, когато throttle.module деактивира обекти с възможност за подсилване (по-бързо изпълнение).','bg','0','0','0'),('2925','\'Родител\' на темата. Може да бъде друга тема или theme engine.','bg','0','0','0'),('2924','Вид на обекта, дали е модул, тема или theme_engine.','bg','0','0','0'),('2923','Името на обекта; например елемент.','bg','0','0','0'),('2922','Пътя до основния файл на този обект, относителен към Drupal коренната директория; например modules/node/node.module.','bg','0','0','0'),('2921','Списък с всички модули, теми и theme engines, които са или са били инста-лирани на файловата система на Drupal.','bg','0','0','0'),('2920','Детайлното съдържание на $_SESSION, списък на име/стойност двойките, които се запазват по време на заявки към страници от този сесиен ID. Drupal зарежда  $_SESSION от тук при старта на всяка заявка и го запазва в края на всяка такава.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2919','Времето когато този потребител е публикувал съдържание последно. Тази опция се използва когато сайта има специфичен minimum_cache_lifetime. Проверете cache_get().','bg','0','0','0'),('2918','Unix времеви маркер кога тази сесия е подала заявка за страница последно. Старите записи се изчистват от PHP автоматично.','bg','0','0','0'),('2917','IP адреса използвал последнот това сесийно ID (sid).','bg','0','0','0'),('2916','Основен ключ: Сесийно ID. Стойността генерирана от PHP сесийното API.','bg','0','0','0'),('2915','{users}.uid съответстващо на сесията, или 0 за анонимени потебители.','bg','0','0','0'),('2914','Обработчиците на сесии в Drupal четат и записват в таблицата със сесии. Всеки запис пред-ставлява единична потебителска сесия, независимо за анонимен или идентифициран потребител.','bg','0','0','0'),('2913','Flag индикираш, че тази връзка е била генерирана в процеса на обновяване от Drupal 5.','bg','0','0','0'),('2912','Деветото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2910','Седмото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2911','Осмото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2909','Шестото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2908','Петото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2907','Четвъртото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2906','Третото ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2905','Второто ID на връзка от менюто в целия път. Виж p1.','bg','0','0','0'),('2904','Първото ID на връзка от менюто в целия път. Ако N = дълбочина, то pN трябва да съвпада с ID-то. Ако дълбочината > 1 то p(N-1) трябва да съвпада с ID на родителската връзка. Всяко pX където X > дълбочина трябва да е равно на нулаКолоните p1 .. p9 се наричат още родителски.','bg','0','0','0'),('2903','Flag индикиращ, дали потебителя е създал или редактирал ръчно връзката (1 = потебителска, 0 = непотребителска).','bg','0','0','0'),('2902','The depth relative to the top level. A link with plid == 0 will have depth == 1.','bg','0','0','0'),('2901','Тежест на връзката в същото меню и на същото ниво.','bg','0','0','0'),('2900','Flad индикиращ дали тази връзка да бъде рендирана като разгъната в менютата - разгърнатите връзки винаги показват своите дъщерни връзки, вместо само когато връзката е в активен режим (1 = разгъната, 0 = неразгъната)','bg','0','0','0'),('2899','Flag индикиращ дали други връзки са дъщерни за тази връзка (1 = дъщерни съществуват, 0 = няма дъщерни).','bg','0','0','0'),('2898','Flag индикиращ, дали хипервръзката е пълен URL адрес започващ с прото-кол, като http:// (1 = външен, 0 = вътрешен).','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2897','Flag индикиращ дали връзката да бъде рендирана в менюта. (1 = деактивиран елемент от менюто, който се показва само на административните екрани, -1 = callback за меню, 0 = нормална, видима връзка)','bg','0','0','0'),('2896','Името на модула генерариал тази връзка','bg','0','0','0'),('2895','Детайлен списък с опциите, които могат да се използват от url() или l() функциите, като заявки или HTML атрибути.','bg','0','0','0'),('2894','Текста показван за тази връзка и записан в {menu_router}, който може да бъде модифициран от callback-a за заглавие.','bg','0','0','0'),('2893','За връзки, които кореспондират с Drupal пътя (външен = 0), това свързва връзката с {menu_router}.path.','bg','0','0','0'),('2892','Drupal път или външен път, до ресурса на тази хипервръзка.','bg','0','0','0'),('2891','ID-то на родителската връзка представлява ID-то на връзката от менюто, която е на по-високо ниво в йерархията или нула ако връзката е от най-високо ниво в това меню.','bg','0','0','0'),('2890','ID-то на връзката от менюто представлява целия основен ключ.','bg','0','0','0'),('2889','Име на менюто. Всички връзки с едно и също име на менюто (като \'навигация\' например) са част от съответното меню.','bg','0','0','0'),('2888','Съдържа самостоятелните връзки в менюто.','bg','0','0','0'),('2887','Файла за включване в този елемент, обикновено функцията за callback за страница е част от него.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2886','Тежест на елемента. Елементите с по-малка тежест се разполагат по-напред в менюто, а елементите с по-голяма - по-назад.','bg','0','0','0'),('2885','Позицията на блока (ляво или дясно) за този елемент на страницата административната страница.','bg','0','0','0'),('2884','Описание на този обект.','bg','0','0','0'),('2883','Името на функцията използвана за рендирането на блока за този елемент на административната страница.','bg','0','0','0'),('2881','Детайлен списък на аргументите за callback за заглавието. Ако това поле е празно, заглавието ще бъде използвано като основен аргумент за callback-а.','bg','0','0','0'),('2882','Номеративно представяне на вида на обекта от менюто, като MENU_LOCAL_TASK.','bg','0','0','0'),('2879','Заглавието на текущата страница или заглавието на таба ако това е локална задача.','bg','0','0','0'),('2880','Функция която да промени заглавието. По подразбиране е t()','bg','0','0','0'),('2878','Обичаен път до най-близката non-tab родителска страница. За страници, които не са локални задачи, това ще бъде същия път.','bg','0','0','0'),('2875','Номеративно представяне на това, колко специфичен е този път.','bg','0','0','0'),('2876','Брой на частите в този обичаен път.','bg','0','0','0'),('2877','Само за локални задачи (табове) - обичайния път за родителската страница ( може да бъде също локална задача).','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2872','Детайлен списък на аргументите за callback за достъп.','bg','0','0','0'),('2873','Името на функцията рендираща страницата.','bg','0','0','0'),('2874','Детайлен списък на аргументите за callback за страница.','bg','0','0','0'),('2870','Детайлен списък на имената на функциите (като user_current_to_arg), които да заменят част от обичайния път с други стрингове.','bg','0','0','0'),('2871','Callback който определя достъпа до този обичаен път. По подразбиране е user_access.','bg','0','0','0'),('2869','Детайлен списък на имената на функциите (като node_load), които да бъдат извиквани за да се зареди обект отговарящ на част от текущия път.','bg','0','0','0'),('2868','Основен ключ: Drupal пътя, който описва този запис.','bg','0','0','0'),('2867','Посочва пътища към различни callbacks (достъп, страница и заглавие)','bg','0','0','0'),('2866','Unix времеви маркер, кога е бил последния преглед.','bg','0','0','0'),('2865','{Елемента}.nid, който е бил прегледан.','bg','0','0','0'),('2864','{users}.uid който преглежда {node} nid.','bg','0','0','0'),('2863','Запис кои {users} са прегледали съответните {node}.','bg','0','0','0'),('2861','Хост на посетителя.','bg','0','0','0'),('2862','Времеви маркер на събитието.','bg','0','0','0'),('2859','Unique flood event ID.','bg','0','0','0'),('2860','Име на събитие (например контакт).','bg','0','0','0'),('2858','Flood контролира горната граница на събитията, като брой опити за контакт например.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2854','MIME тип на файла','bg','0','0','0'),('2855','Размер на файла в байтове.','bg','0','0','0'),('2856','Flag индикация показваща дали файла е временен (1) или перманентен (0).','bg','0','0','0'),('2857','UNIX времеви маркер показващ кога е добавен файла.','bg','0','0','0'),('2852','Име на файла','bg','0','0','0'),('2853','Път до файла относителен за Drupal коренната директория.','bg','0','0','0'),('2850','Основен ключ: Unique files ID.','bg','0','0','0'),('2851','{users}.uid на потебителя, който е асоцииран с файла.','bg','0','0','0'),('2849','Запазва информация за качените файлове.','bg','0','0','0'),('2848','Кеш таблиза за менютата използвана за запазване на обичайната информация или генерираните структури от връзки за различните меню/страница/потребител комбинации.','bg','0','0','0'),('2847','Кеш таблица използвана за запазване на компресирани страници за анонимните потребители, ако кеширането на страници е активирано.','bg','0','0','0'),('2846','Кеш таблица за системата на формите, в която се запазва информацията от вградените форми в сайта за да послужи при последващи заявки към страниците.','bg','0','0','0'),('2845','Flag индикиращ, дали съдържанието е частично (1) или не (0).','bg','0','0','0'),('2843','Unix времеви маркер показващ, кога е създаден записа в кеша.','bg','0','0','0'),('2844','Всякакви потебителски HTTP хедъри, които трябва да бъдат добавени към кеша.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2841','Събиране на информация за кеширане.','bg','0','0','0'),('2842','UNIX времеви маркер показващ, кога трябва да изтече записа в кеша, или 0 за никога.','bg','0','0','0'),('2840','Основен ключ: Unique cache ID.','bg','0','0','0'),('2838','Детайлен списък съдържащ технологичната информация за batch-а.','bg','0','0','0'),('2839','Обща кешинг таблица за кеширане на информация, която няма собствена, обособена таблица. Допълнителените модули, също могат да използват тази таблица за да кеширват.','bg','0','0','0'),('2834','Записва детайли за процеси, които се изпълняват с няколко HTTP заявки.','bg','0','0','0'),('2835','Основен ключ: Unique batch ID.','bg','0','0','0'),('2836','Стрингов маркер генериран за проверка на съответствието между id-то на сесията на текущия потребител и id-то на batch-a за да може само този потребител, който е създал batch-a да има достъп до него.','bg','0','0','0'),('2837','Unix времеви маркер показващ, кога един batch е насрочен за изпълнение. Изпълнените batch-ове се изчистват периодично с cron.','bg','0','0','0'),('2833','Записва ID-та за действия не по-подразбиране.','bg','0','0','0'),('2830','Функцията, която ще се извика след като това действие бива стартирано.','bg','0','0','0'),('2831','Параметри, които да бъдат подадени на функцията.','bg','0','0','0'),('2832','Описание на действието.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2826','Стойността на променливата','bg','0','0','0'),('2827','Записва информация за действие.','bg','0','0','0'),('2828','Основен ключ: Unique actions ID.','bg','0','0','0'),('2829','Обекта над който въздейства това действие (елемент, потебител, комантар, системни или потребителски тъпове съдържание.)','bg','0','0','0'),('2825','Името на променливата','bg','0','0','0'),('2820','Старт cron','bg','0','0','0'),('2821','SQL','bg','0','0','0'),('2822','система','bg','0','0','0'),('2823','заключителна част','bg','0','0','0'),('2824','Дойки променлива/стойност създадени от ядрото на Drupal или всеки друг модул или тема. Всички променливи са кеширани в паметта при всяка заявка към Drupal и разработчиците трябва да обърнат внимание какво е записано тук.','bg','0','0','0'),('2818','Доклад за състоянието','bg','0','0','0'),('2819','Състави доклад на състоянието, работата на сайта и на всички открити грешки.','bg','0','0','0'),('2816','Доклади','bg','0','0','0'),('2817','Преглед на системни журнали и друга информация за състоянието.','bg','0','0','0'),('2815','Тестова заявка','bg','0','0','0'),('2814','Проверка за прости URL-и','bg','0','0','0'),('2813','Активиране или деактивиране на прости URL-и на сайта.','bg','0','0','0'),('2810','Преглед на дата и час','bg','0','0','0'),('2811','Поддръжка на сайта','bg','0','0','0'),('2812','Сваляне на сайта за профилактика или повторно публикуване в мрежата.','bg','0','0','0');
INSERT INTO `locales_target` VALUES ('2809','Настройки на извеждането на датата и часа в Drupal, както и системна часова зона по подразбиране.','bg','0','0','0'),('2803','Укажи на Drupal къде да съхранява качените файлове и как да се достъпват.','bg','0','0','0'),('2804','Инструменти за изображения','bg','0','0','0'),('2805','Избор на инструменти за изображения, ако сте инсталирали допълнителни инструменти.','bg','0','0','0'),('2806','RSS публикуване','bg','0','0','0'),('2807','Определяне на броя обекти за източник и начин на описание на източници - заглавие/резюме/цял текст.','bg','0','0','0'),('2808','Дата и час','bg','0','0','0'),('2801','Производителност','bg','0','0','0'),('2802','Активиране или деактивиране на кеширането на страници за анонимни потребители и настройка на опциите за CSS и JS трафик оптимизация.','bg','0','0','0'),('2800','Настройки на модулите за водене на журнали и предупреждения. Различните модули могат да пренасочват резултата от Drupal системните събития към различни направления като системни журнали, база данни, мейл .... и др.','bg','0','0','0'),('2799','Водене на журнали и предупреждения','bg','0','0','0'),('2797','Промяна на основната информация за сайта, като име на сайта, мото, мейл адрес, мисия, заглавна страница и др.','bg','0','0','0'),('2798','Управление работата на Drupal при грешки, включващи грешка 403/404, както и оповестяване на PHP грешки.','bg','0','0','0');
