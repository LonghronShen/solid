INSERT INTO `locales_source` VALUES ('1562',' modules/filter/filter.module:250','default','Quotation mark','none'),('1563',' modules/filter/filter.module:252','default','Character Description','none'),('1564',' modules/filter/filter.module:266','default','No HTML tags allowed','none'),('1565',' modules/filter/filter.module:274','default','Lines and paragraphs break automatically.','6.16'),('1566',' modules/filter/filter.module:276','default','Lines and paragraphs are automatically recognized. The &lt;br /&gt; line break, &lt;p&gt; paragraph and &lt;/p&gt; close paragraph tags are inserted automatically. If paragraphs are not recognized simply add a couple blank lines.','none'),('1567',' modules/filter/filter.module:280','default','Web page addresses and e-mail addresses turn into links automatically.','6.16'),('1568',' modules/filter/filter.module:484','default','Input format','6.16'),('1569',' modules/filter/filter.module:604;655','default','HTML filter','6.16'),('1570',' modules/filter/filter.module:604','default','Line break converter','6.16'),('1571',' modules/filter/filter.module:604;715','default','URL filter','6.16'),('1572',' modules/filter/filter.module:604','default','HTML corrector','6.16'),('1573',' modules/filter/filter.module:609','default','Allows you to restrict whether users can post HTML and which tags to filter out. It will also remove harmful content such as JavaScript events, JavaScript URLs and CSS styles from those tags that are not removed.','none'),('1574',' modules/filter/filter.module:611','default','Converts line breaks into HTML (i.e. &lt;br&gt; and &lt;p&gt; tags).','none'),('1575',' modules/filter/filter.module:613','default','Turns web and e-mail addresses into clickable links.','none'),('1576',' modules/filter/filter.module:615','default','Corrects faulty and chopped off HTML in postings.','none'),('1577',' modules/filter/filter.module:660','default','Filter HTML tags','none'),('1578',' modules/filter/filter.module:662','default','St