INSERT INTO `locales_source` VALUES ('2818',' modules/system/system.module:478','default','Status report','6.16'),('2819',' modules/system/system.module:479','default','Get a status report about your site\'s operation and any detected problems.','6.16'),('2820',' modules/system/system.module:486','default','Run cron','6.16'),('2821',' modules/system/system.module:498','default','SQL','6.16'),('2822',' modules/system/system.module:0','default','system','6.16'),('2823',' modules/system/system.module:0','default','Footer','6.16'),('2824',' modules/system/system.install:417','default','Named variable/value pairs created by Drupal core or any other module or theme. All variables are cached in memory at the start of every Drupal request so developers should not be careless about what is stored here.','none'),('2825',' modules/system/system.install:420','default','The name of the variable.','none'),('2826',' modules/system/system.install:426','default','The value of the variable.','none'),('2827',' modules/system/system.install:435','default','Stores action information.','none'),('2828',' modules/system/system.install:438;474','default','Primary Key: Unique actions ID.','none'),('2829',' modules/system/system.install:444','default','The object that that action acts on (node, user, comment, system or custom types.)','none'),('2830',' modules/system/system.install:450','default','The callback function that executes when the action runs.','none'),('2831',' modules/system/system.install:456','default','Parameters to be passed to the callback function.','none'),('2832',' modules/system/system.install:461','default','Description of the action.','none'),('2833',' modules/system/system.install:471','default','Stores action IDs for non-default actions.','none'),('2834',' modules/system/system.install:483','default','Stores details about batches (processes that run in multiple HTTP requests).','6.16'),('2835',' modules/system/system.install:486','default','Primary Key: Unique batch 