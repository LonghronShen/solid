INSERT INTO `locales_source` VALUES ('2488',' modules/statistics/statistics.module:0','default','statistics','6.16'),('2489',' modules/statistics/statistics.install:52','default','Stores site access information for statistics.','none'),('2490',' modules/statistics/statistics.install:57','default','Primary Key: Unique accesslog ID.','none'),('2491',' modules/statistics/statistics.install:64','default','Browser session ID of user that visited page.','6.16'),('2492',' modules/statistics/statistics.install:70','default','Title of page visited.','6.16'),('2493',' modules/statistics/statistics.install:76','default','Internal path to page visited (relative to Drupal root.)','6.16'),('2494',' modules/statistics/statistics.install:82','default','Referrer URI.','6.16'),('2495',' modules/statistics/statistics.install:88','default','Hostname of user that visited the page.','6.16'),('2496',' modules/statistics/statistics.install:95','default','User {users}.uid that visited the page.','none'),('2497',' modules/statistics/statistics.install:102','default','Time in milliseconds that the page took to load.','6.16'),('2498',' modules/statistics/statistics.install:109','default','Timestamp of when the page was visited.','6.16'),('2499',' modules/syslog/syslog.module:22','default','The syslog module enables Drupal to send messages to the operating system\'s logging facility.','none'),('2500',' modules/syslog/syslog.module:23','default','Syslog is an operating system administrative logging tool, and provides valuable information for use in system management and security auditing. Most suited to medium and large sites, syslog provides filtering tools that allow messages to be routed by type and severity. On UNIX/Linux systems, the file /etc/syslog.conf defines this routing configuration; on Microsoft Windows, all messages are sent to the Event Log. For more information on syslog facilities, severity levels, and how to set up a syslog.conf file, see <a href=\"@syslog_conf\">UNIX/Linux syslog.conf</a> and PHP\'s <a href=\"@php_openlog\">openlog</a> and <a href=\"@php_syslog\">syslog</a> functions.','none');
INSERT INTO `locales_source` VALUES ('2501',' modules/syslog/syslog.module:24','default','For more information, see the online handbook entry for <a href=\"@syslog\">Syslog module</a>.','none'),('2502',' modules/syslog/syslog.module:42','default','Send events to this syslog facility','none'),('2503',' modules/syslog/syslog.module:45','default','Select the syslog facility code under which Drupal\'s messages should be sent. On UNIX/Linux systems, Drupal can flag its messages with the code LOG_LOCAL0 through LOG_LOCAL7; for Microsoft Windows, all messages are flagged with the code LOG_USER. Depending on the system configuration, syslog and other logging tools use this code to identify or filter Drupal messages from within the entire system log. For more information on syslog, see <a href=\"@syslog_help\">Syslog help</a>.','none'),('2504',' modules/syslog/syslog.module:55','default','LOG_USER - User level messages. Use this for Windows.','none'),('2505',' modules/syslog/syslog.module:59','default','LOG_LOCAL0 - Local 0','none'),('2506',' modules/syslog/syslog.module:60','default','LOG_LOCAL1 - Local 1','none'),('2507',' modules/syslog/syslog.module:61','default','LOG_LOCAL2 - Local 2','none'),('2508',' modules/syslog/syslog.module:62','default','LOG_LOCAL3 - Local 3','none'),('2509',' modules/syslog/syslog.module:63','default','LOG_LOCAL4 - Local 4','none'),('2510',' modules/syslog/syslog.module:64','default','LOG_LOCAL5 - Local 5','none'),('2511',' modules/syslog/syslog.module:65','default','LOG_LOCAL6 - Local 6','none'),('2512',' modules/syslog/syslog.module:66','default','LOG_LOCAL7 - Local 7','none'),('2513',' modules/syslog/syslog.module:32','default','Settings for syslog logging. Syslog is an operating system administrative logging tool used in systems management and security auditing. Most suited to medium and large sites, syslog provides filtering tools that allow messages to be routed by type and severity.','none'),('2514',' modules/syslog/syslog.module:0','default','syslog','none'),('2515',' modules/system/system.admin.inc:22','default','One or more problems were detected with your Drupal installation. Check the <a href=\"@status\">status report</a> for more information.','6.16');
