INSERT INTO `locales_source` VALUES ('1248',' modules/comment/comment.admin.inc:190','default','Delete comments','none'),('1249',' modules/comment/comment.admin.inc:207','default','The comments have been deleted.','none'),('1250',' modules/comment/comment.admin.inc:228','default','The comment no longer exists.','none'),('1251',' modules/comment/comment.admin.inc:245','default','Are you sure you want to delete the comment %title?','none'),('1252',' modules/comment/comment.admin.inc:247','default','Any replies to this comment will be lost. This action cannot be undone.','none'),('1253',' modules/comment/comment.admin.inc:257','default','The comment and all its replies have been deleted.','none'),('1254',' modules/comment/comment.admin.inc:111 modules/comment/comment.module:707','default','Comment: updated %subject.','none'),('1255',' modules/comment/comment.admin.inc:280','default','Cannot delete non-existent comment.','none'),('1256',' modules/comment/comment.admin.inc:286','default','Comment: deleted %subject.','none'),('1257',' modules/comment/comment.pages.inc:65;105','default','You are not authorized to post comments.','none'),('1258',' modules/comment/comment.pages.inc:78;87','default','The comment you are replying to does not exist.','none'),('1259',' modules/comment/comment.pages.inc:98','default','This discussion is closed: you can\'t post new comments.','none'),('1260',' modules/comment/comment.pages.inc:102','default','Reply','none'),('1261',' modules/comment/comment.pages.inc:111','default','You are not authorized to view comments.','none'),('1262',' modules/comment/comment.module:129','default','The comment module allows visitors to comment on your posts, creating ad hoc discussion boards. Any <a href=\"@content-type\">content type</a> may have its <em>Default comment setting</em> set to <em>Read/Write</em> to allow comments, or <em>Disabled</em>, to prevent comments. Comment display settings and other controls may also be customized for each content type (some display settings are customizable by individual users).','6.16'),('1263',' modules/comment/comment.module:130','default','Comment permissions are assigned to user roles, and are used to determine whether anonymous users (or other roles) are allowed to comment on posts. If anonymous users are allowed to comment, their individual contact information may be retained in cookies stored on their local computer for use in later comment submissions. When a comment has no replies, it may be (optionally) edited by its author. The comment module uses the same input formats and HTML tags available when creating other forms of content.','6.16');
INSERT INTO `locales_source` VALUES ('1264',' modules/comment/comment.module:131','default','For more information, see the online handbook entry for <a href=\"@comment\">Comment module</a>.','6.16'),('1265',' modules/comment/comment.module:134','default','Below is a list of the latest comments posted to your site. Click on a subject to see the comment, the author\'s name to edit the author\'s user information, \'edit\' to modify the text, and \'delete\' to remove their submission.','none'),('1266',' modules/comment/comment.module:136','default','Below is a list of the comments posted to your site that need approval. To approve a comment, click on \'edit\' and then change its \'moderation status\' to Approved. Click on a subject to see the comment, the author\'s name to edit the author\'s user information, \'edit\' to modify the text, and \'delete\' to remove their submission.','none'),('1267',' modules/comment/comment.module:284;288','default','Recent comments','6.16'),('1268',' modules/comment/comment.module:420','default','Jump to the first comment of this posting.','none'),('1269',' modules/comment/comment.module:431','default','Jump to the first new comment of this posting.','none'),('1270',' modules/comment/comment.module:440;461','default','Add new comment','6.16'),('1271',' modules/comment/comment.module:442','default','Add a new comment to this page.','6.16'),('1272',' modules/comment/comment.module:463','default','Share your thoughts and opinions related to this posting.','6.16'),('1273',' modules/comment/comment.module:492;575','default','Comment settings','6.16'),('1274',' modules/comment/comment.module:498','default','Default comment setting','6.16'),('1275',' modules/comment/comment.module:500;584','default','Read only','6.16'),('1276',' modules/comment/comment.module:500;584','default','Read/Write','6.16'),('1277',' modules/comment/comment.module:501','default','Users with the <em>administer comments</em> permission will be able to override this setting.','6.16'),('1278',' modules/comment/comment.module:505','default','Default display mode','6.16');
INSERT INTO `locales_source` VALUES ('1279',' modules/comment/comment.module:508','default','The default view for comments. Expanded views display the body of the comment. Threaded views keep replies together.','6.16'),('1280',' modules/comment/comment.module:512','default','Default display order','6.16'),('1281',' modules/comment/comment.module:515','default','The default sorting for new users and anonymous users while viewing comments. These users may change their view using the comment control panel. For registered users, this change is remembered as a persistent user preference.','6.16'),('1282',' modules/comment/comment.module:519','default','Default comments per page','6.16'),('1283',' modules/comment/comment.module:522','default','Default number of comments for each page: more comments are distributed in several pages.','6.16'),('1284',' modules/comment/comment.module:526','default','Comment controls','6.16'),('1285',' modules/comment/comment.module:529','default','Display above the comments','6.16'),('1286',' modules/comment/comment.module:530','default','Display below the comments','6.16'),('1287',' modules/comment/comment.module:531','default','Display above and below the comments','6.16'),('1288',' modules/comment/comment.module:532','default','Do not display','6.16'),('1289',' modules/comment/comment.module:533','default','Position of the comment controls box. The comment controls let the user change the default display mode and display order of comments.','6.16'),('1290',' modules/comment/comment.module:537','default','Anonymous commenting','6.16'),('1291',' modules/comment/comment.module:540','default','Anonymous posters may not enter their contact information','6.16'),('1292',' modules/comment/comment.module:541','default','Anonymous posters may leave their contact information','6.16'),('1293',' modules/comment/comment.module:542','default','Anonymous posters must leave their contact information','6.16'),('1294',' modules/comment/comment.module:543','